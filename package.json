{
  "name": "com.erdbeerbeet.wordprogressor",
  "displayName": "WordProgressor",
  "version": "0.1.9",
  "description": "Manage your writing projects, stories, submissions and daily writing goal.",
  "main": "index.js",
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Fenja Hanisch",
  "license": "Apache-2.0",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-buildinfo": {},
      "cordova-universal-links-plugin": {},
      "cordova-plugin-browsertab": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "com.firebase.cordova",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      },
      "cordova-plugin-firebase-messaging": {
        "IOS_FIREBASE_POD_VERSION": "~> 8.8.0",
        "ANDROID_FIREBASE_MESSAGING_VERSION": "22.0.+",
        "ANDROIDX_CORE_VERSION": "1.6.+"
      }
    },
    "platforms": [
      "ios",
      "browser",
      "android"
    ]
  },
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./src/assets/scripts/setEnv.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=prod && ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.4",
    "@angular/cdk": "^13.3.4",
    "@angular/common": "~13.3.4",
    "@angular/compiler": "~13.3.4",
    "@angular/core": "~13.3.4",
    "@angular/fire": "^7.3.0",
    "@angular/forms": "~13.3.4",
    "@angular/material": "^13.3.4",
    "@angular/material-moment-adapter": "^13.3.4",
    "@angular/platform-browser": "~13.3.4",
    "@angular/platform-browser-dynamic": "~13.3.4",
    "@angular/platform-server": "~13.3.4",
    "@angular/router": "~13.3.4",
    "@angular/service-worker": "~13.3.4",
    "@material/top-app-bar": "^13.0.0",
    "angular-moment": "^1.3.0",
    "angular-sanitize": "^1.8.3",
    "build-tools": "^2.1.1",
    "cordova": "^11.0.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-browsertab": "0.2.0",
    "cordova-plugin-buildinfo": "4.0.0",
    "cordova-plugin-compat": "1.2.0",
    "cordova-plugin-customurlscheme": "5.0.2",
    "cordova-plugin-inappbrowser": "5.0.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova-universal-links-plugin-fix": "1.2.1",
    "firebase": "^9.6.11",
    "moment": "^2.29.3",
    "ngx-online-status": "^1.0.1",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.3",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^17.0.25",
    "@types/uuid": "^8.3.4",
    "cordova-android": "^9.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-plugin-device": "^2.1.0",
    "cordova-plugin-firebase-analytics": "~6.1.0",
    "cordova-plugin-firebase-messaging": "^6.1.2",
    "cordova-support-android-plugin": "~1.0.0",
    "dotenv": "^16.0.0",
    "execa": "^6.1.0",
    "jasmine-core": "~4.1.0",
    "karma": "^6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.3",
    "yargs": "^17.4.1"
  }
}
